<!--Examples for the AWS PowerShell CmdLets-->
<examples>
  <example>
    <code>$newTag = New-Object -TypeName Amazon.ElasticLoadBalancingV2.Model.Tag
$newTag.Key = 'productVersion'
$newTag.Value = '1.0.0'

Add-ELB2Tag -ResourceArn 'arn:aws:elasticloadbalancing:us-east-1:123456789012:loadbalancer/app/test-alb/3651b4394dd9a24f' -Tag $newTag -Region us-east-1</code>
    <description>This example add new Tag to specified AWS.Tools.ElasticLoadBalancingV2 resource.</description>
  </example>
</examples>