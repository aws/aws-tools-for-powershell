<?xml version="1.0" encoding="utf-8"?>
<examples>
  <example>
    <code>Register-ELBInstanceWithLoadBalancer -LoadBalancerName my-test-loadbalancer -Instance "i-1a2b3c4d"</code>
    <description>Registers the single EC2 instance with the specified load balancer.</description>
  </example>
  <example>
    <code>Register-ELBInstanceWithLoadBalancer -LoadBalancerName my-test-loadbalancer -Instance @("i-1a2b3c4d", "i-4d3c2b1a")</code>
    <description>Registers two EC2 instances with the specified load balancer.</description>
  </example>
</examples>