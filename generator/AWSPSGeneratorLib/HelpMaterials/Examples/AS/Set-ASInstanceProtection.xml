<?xml version="1.0" encoding="utf-8"?>
<examples>
  <example>
    <code>Set-ASInstanceProtection -AutoScalingGroupName my-asg -InstanceId i-12345678 -ProtectedFromScaleIn $true</code>
    <description>This example enables instance protection for the specified instance.</description>
  </example>
  <example>
    <code>Set-ASInstanceProtection -AutoScalingGroupName my-asg -InstanceId i-12345678 -ProtectedFromScaleIn $false</code>
    <description>This example disables instance protection for the specified instance.</description>
  </example>
</examples>