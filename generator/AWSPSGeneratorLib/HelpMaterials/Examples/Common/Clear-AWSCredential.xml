<?xml version="1.0" encoding="utf-8"?>
<examples>
  <example>
    <code>Clear-AWSCredential</code>
    <description>Clears any credentials set as default in the current shell. The stored profiles are not affected.</description>
  </example>
  <example>
    <code>Clear-AWSCredential -ProfileName myCredentials</code>
    <description>Deletes the credential profile named 'myCredentials'.</description>
  </example>
</examples>