<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>DC</ServiceNounPrefix>
  <ServiceName>AWS Direct Connect</ServiceName>
  <ServiceClientInterface>IAmazonDirectConnect</ServiceClientInterface>
  <ServiceClient>AmazonDirectConnectClient</ServiceClient>
  <ServiceNamespace>Amazon.DirectConnect</ServiceNamespace>

  <NounMappings>
    <Map From="Connections" To="Connection" />
    <Map From="Interconnects" To="Interconnect" />
    <Map From="VirtualGateways" To="VirtualGateway" />
    <Map From="VirtualInterfaces" To="VirtualInterface" />
  </NounMappings>

  <AutoIterate Start="NextToken" Next="NextToken" EmitLimit="MaxResults" />

  <ServiceOperations>
    <ServiceOperation MethodName="AllocateConnectionOnInterconnect" Verb="Enable" Output="Response" PipelineParameter="InterconnectId" PositionalParameters="ConnectionName;OwnerAccount;Bandwidth;Vlan"/>
    <ServiceOperation MethodName="AllocateHostedConnection" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="AllocatePrivateVirtualInterface" Verb="Enable" Output="Response" PipelineParameter="ConnectionId" PositionalParameters="OwnerAccount"/>
    <ServiceOperation MethodName="AllocatePublicVirtualInterface" Verb="Enable" Output="Response" PipelineParameter="ConnectionId" PositionalParameters="OwnerAccount"/>
    <ServiceOperation MethodName="AssociateConnectionWithLag" Verb="Register" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="AssociateHostedConnection" Verb="Register" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="AssociateVirtualInterface" Verb="Register" PipelineParameter="VirtualInterfaceId" Output="Response" />
    <ServiceOperation MethodName="ConfirmConnection" />
    <ServiceOperation MethodName="ConfirmPrivateVirtualInterface" PipelineParameter="VirtualInterfaceId" PositionalParameters="VirtualGatewayId"/>
    <ServiceOperation MethodName="ConfirmPublicVirtualInterface" />
    <ServiceOperation MethodName="CreateBGPPeer" PipelineParameter="VirtualInterfaceId"/>
    <ServiceOperation MethodName="CreateConnection" Output="Response" PipelineParameter="ConnectionName" PositionalParameters="Bandwidth;Location"/>
    <ServiceOperation MethodName="CreateDirectConnectGateway" Noun="Gateway" PipelineParameter="DirectConnectGatewayName" />
    <ServiceOperation MethodName="CreateDirectConnectGatewayAssociation" Noun="GatewayAssociation" PipelineParameter="VirtualGatewayId" />
    <ServiceOperation MethodName="CreateInterconnect" Output="Response" PipelineParameter="InterconnectName" PositionalParameters="Bandwidth;Location"/>
    <ServiceOperation MethodName="CreateLag" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="CreatePrivateVirtualInterface" Output="Response" PipelineParameter="ConnectionId" />
    <ServiceOperation MethodName="CreatePublicVirtualInterface" Output="Response" PipelineParameter="ConnectionId"/>
    <ServiceOperation MethodName="DeleteBGPPeer" PipelineParameter="VirtualInterfaceId"/>
    <ServiceOperation MethodName="DeleteConnection" Output="Response" />
    <ServiceOperation MethodName="DeleteDirectConnectGateway" Noun="Gateway" />
    <ServiceOperation MethodName="DeleteDirectConnectGatewayAssociation" Noun="GatewayAssociation" PipelineParameter="VirtualGatewayId" />
    <ServiceOperation MethodName="DeleteInterconnect" />
    <ServiceOperation MethodName="DeleteLag" Output="Response" />
    <ServiceOperation MethodName="DeleteVirtualInterface" />
    <ServiceOperation MethodName="DisassociateConnectionFromLag" Verb="Unregister" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="DescribeConnections" />
    <ServiceOperation MethodName="DescribeConnectionsOnInterconnect" />
    <ServiceOperation MethodName="DescribeConnectionLoa" PipelineParameter="ConnectionId"/>
    <ServiceOperation MethodName="DescribeDirectConnectGatewayAssociations" Noun="GatewayAssociation" PipelineParameter="DirectConnectGatewayId" />
    <ServiceOperation MethodName="DescribeDirectConnectGatewayAttachments" Noun="GatewayAttachment" PipelineParameter="DirectConnectGatewayId" />
    <ServiceOperation MethodName="DescribeDirectConnectGateways" Noun="Gateway" PipelineParameter="DirectConnectGatewayId" />
    <ServiceOperation MethodName="DescribeHostedConnections" Noun="HostedConnection" />
    <ServiceOperation MethodName="DescribeInterconnects" />
    <ServiceOperation MethodName="DescribeInterconnectLoa" PipelineParameter="InterconnectId"/>
    <ServiceOperation MethodName="DescribeLags" Noun="Lag" />
    <ServiceOperation MethodName="DescribeLoa" PipelineParameter="ConnectionId" Output="Response" />
    <ServiceOperation MethodName="DescribeLocations" Noun="Location" LegacyAlias="Get-DCLocations" />
    <ServiceOperation MethodName="DescribeVirtualGateways"/>
    <ServiceOperation MethodName="DescribeVirtualInterfaces" PipelineParameter="ConnectionId" PositionalParameters="VirtualInterfaceId"/>
    <ServiceOperation MethodName="DescribeTags" Verb="Get" Noun="ResourceTag"/>
    <ServiceOperation MethodName="TagResource" Verb="Add" Noun="ResourceTag" PipelineParameter="Resource" Output="Void"/>
    <ServiceOperation MethodName="UpdateLag" PipelineParameter="LagId" Output="Response" />
    <ServiceOperation MethodName="UntagResource" Verb="Remove" Noun="ResourceTag" PipelineParameter="Resource" Output="Void"/>
  </ServiceOperations>
  
  <PipelineByValueProperties>
    <Property>ConnectionId</Property>
    <Property>InterconnectId</Property>
    <Property>VirtualInterfaceId</Property>
  </PipelineByValueProperties>
</ConfigModel>