<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>EB</ServiceNounPrefix>
  <ServiceName>AWS Elastic Beanstalk</ServiceName>
  <ServiceClientInterface>IAmazonElasticBeanstalk</ServiceClientInterface>
  <ServiceClient>AmazonElasticBeanstalkClient</ServiceClient>
  <ServiceNamespace>Amazon.ElasticBeanstalk</ServiceNamespace>

  <PositionalParameters>ApplicationName</PositionalParameters>
 
  <PipelineParameter>ApplicationName</PipelineParameter>
 
  <NounMappings>
    <Map From="Applications" To="Application" />
    <Map From="ApplicationVersions" To="ApplicationVersion" />
    <Map From="AvailableSolutionStacks" To="AvailableSolutionStack" />
    <Map From="ConfigurationOptions" To="ConfigurationOption" />
    <Map From="ConfigurationSettings" To="ConfigurationSetting" />
    <Map From="EnvironmentResources" To="EnvironmentResource" />
    <Map From="Environments" To="Environment" />
    <Map From="Events" To="Event" />
    <Map From="EnvironmentCNAMEs" To="EnvironmentCNAME" />
  </NounMappings>

  <VerbMappings>
    <Map From="List" To="Get" />
    <Map From="Terminate" To="Stop" />
    <Map From="Check" To="Get" />
    <Map From="Retrieve" To="Get" />
    <Map From="Swap" To="Set" />
    <Map From="Validate" To="Test" />
  </VerbMappings>
 
  <AutoIterate EmitLimit="MaxRecords" Start="NextToken" Next="NextToken" ServicePageSize="1000" />
 
  <MetadataProperties>
    <Property>RefreshedAt</Property>
  </MetadataProperties>
 
  <PipelineByValueProperties>
    <Property>NextToken</Property>
    <Property>ApplicationName</Property>
    <Property>SolutionStackName</Property>
    <Property>EnvironmentId</Property>
    <Property>VersionLabel</Property>
    <Property>EnvironmentName</Property>
    <Property>TemplateName</Property>
  </PipelineByValueProperties>
 
  <ServiceOperations>
    <ServiceOperation MethodName="AbortEnvironmentUpdate" Verb="Stop" Output="Void" ShouldProcessTarget="EnvironmentName"/>
    <ServiceOperation MethodName="ApplyEnvironmentManagedAction" Verb="Submit" Output="Response" ShouldProcessTarget="EnvironmentName"/>
    <ServiceOperation MethodName="CheckDNSAvailability" Output="Response" />
    <ServiceOperation MethodName="ComposeEnvironments" Verb="Group" />
    <ServiceOperation MethodName="CreateApplication" PositionalParameters="Description"/>
    <ServiceOperation MethodName="CreateApplicationVersion" PositionalParameters="VersionLabel;Description"/>
    <ServiceOperation MethodName="CreateConfigurationTemplate" Output="Response" PositionalParameters="TemplateName;SolutionStackName;EnvironmentId;Description;SourceConfiguration"/>
    <ServiceOperation MethodName="CreateEnvironment" Output="Response" PositionalParameters="EnvironmentName"/>
    <ServiceOperation MethodName="CreatePlatformVersion" PipelineParameter="PlatformName" Output="Response" />
    <ServiceOperation MethodName="CreateStorageLocation"/>
    <ServiceOperation MethodName="DeleteApplication" Output="Void" ShouldProcessTarget="ApplicationName"/>
    <ServiceOperation MethodName="DeleteApplicationVersion" Output="Void" ShouldProcessTarget="VersionLabel" PositionalParameters="VersionLabel;DeleteSourceBundle"/>
    <ServiceOperation MethodName="DeleteConfigurationTemplate" Output="Void" ShouldProcessTarget="TemplateName" PositionalParameters="TemplateName"/>
    <ServiceOperation MethodName="DeleteEnvironmentConfiguration" Output="Void" ShouldProcessTarget="EnvironmentName" PositionalParameters="EnvironmentName">
      <PassThru>
        <Expression>cmdletContext.EnvironmentName</Expression>
        <Type>System.String</Type>
        <Documentation>Returns the name of the environment that was updated</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="DeletePlatformVersion" ShouldProcessTarget="PlatformArn"/>
    <ServiceOperation MethodName="DescribeApplications" Noun="Application" LegacyAlias="Get-EBApplications"/>
    <ServiceOperation MethodName="DescribeApplicationVersions" PositionalParameters="VersionLabels" Noun="ApplicationVersion" LegacyAlias="Get-EBApplicationVersions"/>
    <ServiceOperation MethodName="DescribeConfigurationOptions" Output="Response" Noun="ConfigurationOption" LegacyAlias="Get-EBConfigurationOptions"/>
    <ServiceOperation MethodName="DescribeConfigurationSettings" Noun="ConfigurationSetting" LegacyAlias="Get-EBConfigurationSettings"/>
    <ServiceOperation MethodName="DescribeEnvironmentHealth" PipelineParameter="EnvironmentId" Output="Response" />
    <ServiceOperation MethodName="DescribeEnvironmentResources" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName" Noun="EnvironmentResource" LegacyAlias="Get-EBEnvironmentResources"/>
    <ServiceOperation MethodName="DescribeEnvironmentManagedActionHistory" />
    <ServiceOperation MethodName="DescribeEnvironmentManagedActions" Noun="EnvironmentManagedAction" />
    <ServiceOperation MethodName="DescribeEnvironments" PositionalParameters="EnvironmentIds;VersionLabel"/>
    <ServiceOperation MethodName="DescribeEvents"/>
    <ServiceOperation MethodName="DescribeInstancesHealth" PipelineParameter="EnvironmentId" Noun="InstanceHealth" />
    <ServiceOperation MethodName="DescribePlatformVersion" Noun="PlatformVersionDetail"/>
    <ServiceOperation MethodName="ListAvailableSolutionStacks" Output="Response" Noun="AvailableSolutionStackList" LegacyAlias="Get-EBAvailableSolutionStack" />
    <ServiceOperation MethodName="ListPlatformVersions" Noun="PlatformVersion"/>
    <ServiceOperation MethodName="ListTagsForResource" Noun="ResourceTag" Output="Response" />
    <ServiceOperation MethodName="RebuildEnvironment" Verb="Start" Noun="EnvironmentRebuild" Output="Void" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName">
      <PassThru>
        <Expression>GetFirstAssignedParameterValue("EnvironmentId", "EnvironmentName")</Expression>
        <Type>System.String</Type>
        <Documentation>Returns the id or name of the environment (depending on which parameter was supplied)</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="RequestEnvironmentInfo" Output="Void" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName;InfoType">
      <PassThru>
        <Expression>GetFirstAssignedParameterValue("EnvironmentId", "EnvironmentName")</Expression>
        <Type>System.String</Type>
        <Documentation>Returns the id or name of the environment (depending on which parameter was supplied)</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="RestartAppServer" Output="Void" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName">
      <PassThru>
        <Expression>GetFirstAssignedParameterValue("EnvironmentId", "EnvironmentName")</Expression>
        <Type>System.String</Type>
        <Documentation>Returns the id or name of the environment (depending on which parameter was supplied)</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="RetrieveEnvironmentInfo" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName;InfoType"/>
    <ServiceOperation MethodName="SwapEnvironmentCNAMEs" Output="Void" PipelineParameter="SourceEnvironmentId" PositionalParameters="SourceEnvironmentName;DestinationEnvironmentId;DestinationEnvironmentName" Noun="EnvironmentCNAME" LegacyAlias="Set-EBEnvironmentCNAMEs"/>
    <ServiceOperation MethodName="TerminateEnvironment" Output="Response" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName;TerminateResources"/>
    <ServiceOperation MethodName="UpdateApplication" PositionalParameters="Description"/>
    <ServiceOperation MethodName="UpdateApplicationResourceLifecycle" Output="Response" />
    <ServiceOperation MethodName="UpdateApplicationVersion" PositionalParameters="VersionLabel;Description"/>
    <ServiceOperation MethodName="UpdateConfigurationTemplate" Output="Response" PositionalParameters="TemplateName;Description;OptionSettings;OptionsToRemove"/>
    <ServiceOperation MethodName="UpdateEnvironment" Output="Response" PipelineParameter="EnvironmentId" PositionalParameters="EnvironmentName;VersionLabel;TemplateName;Description"/>
    <ServiceOperation MethodName="ValidateConfigurationSettings" PositionalParameters="OptionSettings" Noun="ConfigurationSetting" LegacyAlias="Test-EBConfigurationSettings"/>
    <ServiceOperation MethodName="UpdateTagsForResource" Noun="ResourceTag" PipelineParameter="ResourceArn" Output="Void" />
  </ServiceOperations>
</ConfigModel>