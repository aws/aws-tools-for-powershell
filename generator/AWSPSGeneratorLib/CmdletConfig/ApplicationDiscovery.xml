<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>ADS</ServiceNounPrefix>
  <ServiceName>Application Discovery Service</ServiceName>
  <ServiceClientInterface>IAmazonApplicationDiscoveryService</ServiceClientInterface>
  <ServiceClient>AmazonApplicationDiscoveryServiceClient</ServiceClient>
  <ServiceNamespace>Amazon.ApplicationDiscoveryService</ServiceNamespace>

  <NounMappings>
    <Map From="Tags" To="Tag" />
    <Map From="Agents" To="Agent" />
    <Map From="Configurations" To="Configuration" />
  </NounMappings>
  
  <AutoIterate EmitLimit="MaxResults" Start="NextToken" Next="NextToken" ServicePageSize="100" Exclusions="ListServerNeighbors"/>
  
  <PipelineParameter>ConfigurationIds</PipelineParameter>
  
  <PipelineByValueProperties>
    <Property>AgentId</Property>
    <Property>ConfigurationId</Property>
  </PipelineByValueProperties>
  
  <ServiceOperations>
    <ServiceOperation MethodName="AssociateConfigurationItemsToApplication" Verb="Add" PipelineParameter="ApplicationConfigurationId" Output="Void"/>
    <ServiceOperation MethodName="CreateApplication"/>
    <ServiceOperation MethodName="CreateTags" Output="Void" />
    <ServiceOperation MethodName="DeleteApplications" Noun="Application" Output="Void" LegacyAlias="Remove-ADSApplications"/>
    <ServiceOperation MethodName="DeleteTags" Output="Void" />
    <ServiceOperation MethodName="DescribeAgents" />
    <ServiceOperation MethodName="DescribeConfigurations" />
    <ServiceOperation MethodName="DescribeExportConfigurations" Noun="ExportConfiguration" />
    <ServiceOperation MethodName="DescribeExportTasks" Noun="ExportTask" />
    <ServiceOperation MethodName="DisassociateConfigurationItemsFromApplication" Verb="Remove" PipelineParameter="ApplicationConfigurationId" Output="Void"/>
    <ServiceOperation MethodName="DescribeTags" NoPipelineParameter="true" />
    <ServiceOperation MethodName="ExportConfigurations" Verb="Get" Noun="ExportConfigurationsId" />
    <ServiceOperation MethodName="GetDiscoverySummary" Output="Response"/>
    <ServiceOperation MethodName="ListConfigurations" Verb="Get" Noun="ConfigurationList" NoPipelineParameter="true" />
    <ServiceOperation MethodName="ListServerNeighbors" Verb="Get" Noun="ServerNeighborList" Output="Response"/>
    <ServiceOperation MethodName="StartDataCollectionByAgentIds" Noun="DataCollectionByAgentId" />
    <ServiceOperation MethodName="StartExportTask" AnonymousShouldProcessTarget="true" PipelineParameter="ExportDataFormat" />
    <ServiceOperation MethodName="StopDataCollectionByAgentIds" Noun="DataCollectionByAgentId" />
    <ServiceOperation MethodName="UpdateApplication" PipelineParameter="ConfigurationId" Output="Void"/>
  </ServiceOperations>
</ConfigModel>