<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>CS</ServiceNounPrefix>
  <ServiceName>Amazon CloudSearch</ServiceName>
  <ServiceClientInterface>IAmazonCloudSearch</ServiceClientInterface>
  <ServiceClient>AmazonCloudSearchClient</ServiceClient>
  <ServiceNamespace>Amazon.CloudSearch</ServiceNamespace>

  <PositionalParameters>DomainName</PositionalParameters>
  
  <PipelineParameter>DomainName</PipelineParameter>
  
  <NounMappings>
    <Map From="AnalysisSchemes" To="AnalysisScheme" />
    <Map From="AvailabilityOptions" To="AvailabilityOption" />
    <Map From="Domains" To="Domain" />
    <Map From="DomainNames" To="DomainName" />
    <Map From="Expressions" To="Expression" />
    <Map From="IndexFields" To="IndexField" />
    <Map From="ScalingParameters" To="ScalingParameter" />
    <Map From="ServiceAccessPolicies" To="ServiceAccessPolicy" />
    <Map From="Suggesters" To="Suggester" />
  </NounMappings>
  
  <VerbMappings>
    <Map From="Define" To="Set" />
  </VerbMappings>
  
  <CustomAliases>
    <AliasSet Cmdlet="Set-CSIndexField">Define-CSIndexField</AliasSet>
    <AliasSet Cmdlet="Start-CSIndex">Index-CSDocuments</AliasSet>
  </CustomAliases>
  
  <ServiceOperations>
    <ServiceOperation MethodName="BuildSuggesters" Verb="Start" Noun="SuggestersBuild"/>
    <ServiceOperation MethodName="CreateDomain"/>
    <ServiceOperation MethodName="DefineAnalysisScheme" ShouldProcessTarget="DomainName"/>
    <ServiceOperation MethodName="DefineExpression"/>
    <ServiceOperation MethodName="DefineIndexField"/>
    <ServiceOperation MethodName="DefineSuggester"/>
    <ServiceOperation MethodName="DeleteAnalysisScheme" ShouldProcessTarget="AnalysisSchemeName" PositionalParameters="AnalysisSchemeName"/>
    <ServiceOperation MethodName="DeleteDomain"/>
    <ServiceOperation MethodName="DeleteExpression" ShouldProcessTarget="ExpressionName" PositionalParameters="ExpressionName"/>
    <ServiceOperation MethodName="DeleteIndexField" ShouldProcessTarget="IndexFieldName" PositionalParameters="IndexFieldName"/>
    <ServiceOperation MethodName="DeleteSuggester" ShouldProcessTarget="SuggesterName" PositionalParameters="SuggesterName"/>
    <ServiceOperation MethodName="DescribeAnalysisSchemes" Noun="AnalysisScheme" LegacyAlias="Get-CSAnalysisSchemes"/>
    <ServiceOperation MethodName="DescribeAvailabilityOptions" Noun="AvailabilityOption" LegacyAlias="Get-CSAvailabilityOptions"/>
    <ServiceOperation MethodName="DescribeDomains" />
    <ServiceOperation MethodName="DescribeExpressions"/>
    <ServiceOperation MethodName="DescribeIndexFields" PositionalParameters="FieldNames" Noun="IndexField" LegacyAlias="Get-CSIndexFields"/>
    <ServiceOperation MethodName="DescribeScalingParameters" Noun="ScalingParameter" LegacyAlias="Get-CSScalingParameters"/>
    <ServiceOperation MethodName="DescribeServiceAccessPolicies" Noun="ServiceAccessPolicy" LegacyAlias="Get-CSServiceAccessPolicies"/>
    <ServiceOperation MethodName="DescribeSuggesters"/>
    <ServiceOperation MethodName="IndexDocuments" Verb="Start" Noun="Index"/>
    <ServiceOperation MethodName="ListDomainNames" Verb="Get" Noun="DomainNameList" LegacyAlias="Get-CSListDomainNames"/>
    <ServiceOperation MethodName="UpdateAvailabilityOptions" Noun="AvailabilityOption" LegacyAlias="Update-CSAvailabilityOptions"/>
    <ServiceOperation MethodName="UpdateScalingParameters" Noun="ScalingParameter" LegacyAlias="Update-CSScalingParameters"/>
    <ServiceOperation MethodName="UpdateServiceAccessPolicies" PositionalParameters="AccessPolicies" Noun="ServiceAccessPolicy" LegacyAlias="Update-CSServiceAccessPolicies"/>
  </ServiceOperations>
  
  <PipelineByValueProperties>
    <Property>DomainName</Property>
    <Property>DomainId</Property>
    <Property>IndexFieldName</Property>
  </PipelineByValueProperties>
</ConfigModel>