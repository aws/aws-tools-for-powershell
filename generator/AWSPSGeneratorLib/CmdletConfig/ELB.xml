<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>ELB</ServiceNounPrefix>
  <ServiceName>Elastic Load Balancing</ServiceName>
  <ServiceClientInterface>IAmazonElasticLoadBalancing</ServiceClientInterface>
  <ServiceClient>AmazonElasticLoadBalancingClient</ServiceClient>
  <ServiceNamespace>Amazon.ElasticLoadBalancing</ServiceNamespace>

  <PositionalParameters>LoadBalancerName</PositionalParameters>
  
  <PipelineParameter>LoadBalancerName</PipelineParameter>
  
  <AutoIterate Start="Marker" Next="NextMarker" EmitLimit="PageSize"/>
  
  <NounMappings>
    <Map From="LoadBalancerToSubnets" To="LoadBalancerToSubnet" />
    <Map From="LoadBalancerFromSubnets" To="LoadBalancerFromSubnet" />
    <Map From="LoadBalancerPolicies" To="LoadBalancerPolicy" />
    <Map From="LoadBalancerPolicyTypes" To="LoadBalancerPolicyType" />
    <Map From="LoadBalancerAttributes" To="LoadBalancerAttribute" />
    <Map From="LoadBalancers" To="LoadBalancer" />
    <Map From="LoadBalancerListeners" To="LoadBalancerListener" />
    <Map From="SecurityGroupsToLoadBalancer" To="SecurityGroupToLoadBalancer" />
    <Map From="InstancesWithLoadBalancer" To="InstanceWithLoadBalancer" />
    <Map From="InstancesFromLoadBalancer" To="InstanceFromLoadBalancer" />
    <Map From="LoadBalancerPoliciesForBackendServer" To="LoadBalancerPolicyForBackendServer" />
    <Map From="LoadBalancerPoliciesOfListener" To="LoadBalancerPolicyOfListener" />
    <Map From="AvailabilityZonesForLoadBalancer" To="AvailabilityZoneForLoadBalancer" />
  </NounMappings>
  
  <VerbMappings>
    <Map From="Apply" To="Join" />
    <Map From="Configure" To="Set" />
    <Map From="Deregister" To="Remove" />
  </VerbMappings>
  
  <ServiceOperations>
    <ServiceOperation MethodName="AddTags" Output="Void" PipelineParameter="LoadBalancerNames" PositionalParameters="Tags" Noun="ResourceTag" LegacyAlias="Add-ELBTags">
      <PassThru>
        <Expression>cmdletContext.Tags</Expression>
        <Documentation>Returns the collection of Tag objects that were added</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="ApplySecurityGroupsToLoadBalancer" PositionalParameters="SecurityGroups"/>
    <ServiceOperation MethodName="AttachLoadBalancerToSubnets" PositionalParameters="Subnets"/>
    <ServiceOperation MethodName="ConfigureHealthCheck"/>
    <ServiceOperation MethodName="CreateAppCookieStickinessPolicy" Output="Void" PositionalParameters="PolicyName;CookieName"/>
    <ServiceOperation MethodName="CreateLBCookieStickinessPolicy" Output="Void" PositionalParameters="PolicyName;CookieExpirationPeriod"/>
    <ServiceOperation MethodName="CreateLoadBalancer" PositionalParameters="Listeners;AvailabilityZones"/>
    <ServiceOperation MethodName="CreateLoadBalancerListeners" Output="Void" PositionalParameters="Listeners">
      <PassThru>
        <Expression>cmdletContext.Listeners</Expression>
        <Documentation>Returns the collection of Listener objects that were created</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateLoadBalancerPolicy" Output="Void" PositionalParameters="PolicyName;PolicyTypeName;PolicyAttributes"/>
    <ServiceOperation MethodName="DeleteLoadBalancer" Output="Void"/>
    <ServiceOperation MethodName="DeleteLoadBalancerListeners" Output="Void" ShouldProcessTarget="LoadBalancerPorts" PositionalParameters="LoadBalancerPorts"/>
    <ServiceOperation MethodName="DeleteLoadBalancerPolicy" Output="Void" ShouldProcessTarget="PolicyName" PositionalParameters="PolicyName"/>
    <ServiceOperation MethodName="DeregisterInstancesFromLoadBalancer" ShouldProcessTarget="Instances" PositionalParameters="Instances"/>
    <ServiceOperation MethodName="DescribeAccountLimits" Noun="AccountLimit" />
    <ServiceOperation MethodName="DescribeInstanceHealth" PositionalParameters="Instances"/>
    <ServiceOperation MethodName="DescribeLoadBalancerAttributes" />
    <ServiceOperation MethodName="DescribeLoadBalancerPolicies" PositionalParameters="PolicyNames"/>
    <ServiceOperation MethodName="DescribeLoadBalancerPolicyTypes" />
    <ServiceOperation MethodName="DescribeLoadBalancers" PipelineParameter="LoadBalancerNames"/>
    <ServiceOperation MethodName="DescribeTags" Noun="ResourceTag" LegacyAlias="Get-ELBTags" />
    <ServiceOperation MethodName="DetachLoadBalancerFromSubnets" PositionalParameters="Subnets"/>
    <ServiceOperation MethodName="DisableAvailabilityZonesForLoadBalancer" PositionalParameters="AvailabilityZones"/>
    <ServiceOperation MethodName="EnableAvailabilityZonesForLoadBalancer" PositionalParameters="AvailabilityZones"/>
    <ServiceOperation MethodName="ModifyLoadBalancerAttributes" Output="Response" PositionalParameters="LoadBalancerAttributes" />
    <ServiceOperation MethodName="RegisterInstancesWithLoadBalancer" PositionalParameters="Instances"/>
    <ServiceOperation MethodName="RemoveTags" Output="Void" ShouldProcessTarget="Tags" PipelineParameter="LoadBalancerNames" PositionalParameters="Tags" Noun="ResourceTag" LegacyAlias="Remove-ELBTags">
      <PassThru>
        <Expression>cmdletContext.Tags</Expression>
        <Documentation>Returns the collection of tag keys that were removed</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="SetLoadBalancerListenerSSLCertificate" Output="Void" PositionalParameters="LoadBalancerPort;SSLCertificateId"/>
    <ServiceOperation MethodName="SetLoadBalancerPoliciesForBackendServer" Output="Void" PositionalParameters="InstancePort;PolicyNames"/>
    <ServiceOperation MethodName="SetLoadBalancerPoliciesOfListener" Output="Void" PositionalParameters="LoadBalancerPort;PolicyNames"/>
  </ServiceOperations>
  
  <PipelineByValueProperties>
    <Property>LoadBalancerName</Property>
    <Property>PolicyName</Property>
  </PipelineByValueProperties>
</ConfigModel>