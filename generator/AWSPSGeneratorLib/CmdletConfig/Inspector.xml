<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <ServiceNounPrefix>INS</ServiceNounPrefix>
  <ServiceName>Amazon Inspector</ServiceName>
  <ServiceNamespace>Amazon.Inspector</ServiceNamespace>
  <ServiceClientInterface>IAmazonInspector</ServiceClientInterface>
  <ServiceClient>AmazonInspectorClient</ServiceClient>

  <AutoIterate EmitLimit="MaxResults" Start="NextToken" Next="NextToken"  />

  <VerbMappings>
      <Map From="List" To="Get"/>
  </VerbMappings>
   
  <ServiceOperations>
    <ServiceOperation MethodName="AddAttributesToFindings" />
    <ServiceOperation MethodName="CreateAssessmentTarget" />
    <ServiceOperation MethodName="CreateAssessmentTemplate" />
    <ServiceOperation MethodName="CreateResourceGroup" />
    <ServiceOperation MethodName="DeleteAssessmentRun" />
    <ServiceOperation MethodName="DeleteAssessmentTarget" />
    <ServiceOperation MethodName="DeleteAssessmentTemplate" />
    <ServiceOperation MethodName="DescribeAssessmentRuns" />
    <ServiceOperation MethodName="DescribeAssessmentTargets" />
    <ServiceOperation MethodName="DescribeAssessmentTemplates" />
    <ServiceOperation MethodName="DescribeCrossAccountAccessRole" />
    <ServiceOperation MethodName="DescribeFindings" />
    <ServiceOperation MethodName="DescribeResourceGroups" />
    <ServiceOperation MethodName="DescribeRulesPackages" />
    <ServiceOperation MethodName="GetTelemetryMetadata" />
    <ServiceOperation MethodName="ListAssessmentRunAgents" />
    <ServiceOperation MethodName="ListAssessmentRuns" />
    <ServiceOperation MethodName="ListAssessmentTargets" />
    <ServiceOperation MethodName="ListAssessmentTemplates" />
    <ServiceOperation MethodName="ListEventSubscriptions" />
    <ServiceOperation MethodName="ListFindings" />
    <ServiceOperation MethodName="ListRulesPackages" />
    <ServiceOperation MethodName="ListTagsForResource" />
    <ServiceOperation MethodName="PreviewAgents" />
    <ServiceOperation MethodName="RegisterCrossAccountAccessRole" />
    <ServiceOperation MethodName="RemoveAttributesFromFindings" />
    <ServiceOperation MethodName="SetTagsForResource" />
    <ServiceOperation MethodName="StartAssessmentRun" />
    <ServiceOperation MethodName="StopAssessmentRun" />
    <ServiceOperation MethodName="SubscribeToEvent" />
    <ServiceOperation MethodName="UnsubscribeFromEvent" />
    <ServiceOperation MethodName="UpdateAssessmentTarget" />
  </ServiceOperations>
  
</ConfigModel>