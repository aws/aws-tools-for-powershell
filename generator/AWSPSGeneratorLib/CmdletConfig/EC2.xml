    <ServiceOperation MethodName="EnableVpcClassicLink" />

    <ServiceOperation MethodName="DisableVpcClassicLinkDnsSupport" />
    <ServiceOperation MethodName="EnableVpcClassicLinkDnsSupport" />
    <ServiceOperation MethodName="DescribeVpcClassicLinkDnsSupport" />

