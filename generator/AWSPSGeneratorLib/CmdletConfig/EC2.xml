  <!-- define zero or more custom aliases here; these are emitted in addition to automatic
       noun-operation aliases
  -->
  <CustomAliases>
    <AliasSet Cmdlet="Stop-EC2Instance">EC2-StopInstance</AliasSet>
  </CustomAliases>

  <!-- verb mappings contains service-specific verb remaps, augmenting (or overriding) the global map
  -->
  <VerbMappings>
    <Map From="Associate" To="Register" />
    <Map From="Disassociate" To="Unregister" />
    <Map From="Replace" To="Set" />
    <Map From="Report" To="Send" />
    <Map From="Authorize" To="Grant" />
    <Map From="Assign" To="Register" />
    <Map From="Unassign" To="Unregister" />
    <Map From="Deregister" To="Unregister" />
    <Map From="Reboot" To="Restart" />
    <Map From="Release" To="Remove" />
    <Map From="Run" To="New" />
  </VerbMappings>

  <MetadataProperties>
    <Property>SpotFleetRequestId</Property>
  </MetadataProperties>

  <AutoIterate EmitLimit="MaxResults" Start="NextToken" Next="NextToken" Exclusions="DescribeSpotFleetRequestHistory" />

