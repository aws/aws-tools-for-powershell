        <!-- maintain compat with v1 sdk name -->
        <Param Name="IcmpTypeCode_Code" Alias="Icmp_Code" />
        <Param Name="IcmpTypeCode_Type" Alias="Icmp_Type" />
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="ReplaceRoute" Output="Void" PipelineParameter="RouteTableId"
