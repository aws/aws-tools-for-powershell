                      Noun="SpotFleetRequest"
                      PipelineParameter="SpotFleetRequestId" />
    <ServiceOperation MethodName="RequestSpotFleet"
                      ShouldProcessTarget="SpotFleetRequestConfig_SpotPrice"
                      Output="Void"/>
    <ServiceOperation MethodName="ModifySpotFleetRequest" />

    <ServiceOperation MethodName="ModifyImageAttribute" Output="Void" PipelineParameter="ImageId"/>
    <ServiceOperation MethodName="ModifyInstanceAttribute" Output="Void">
      <Params>
        <Param Name="Groups" Alias="GroupId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="ModifyNetworkInterfaceAttribute" Output="Void" PipelineParameter="NetworkInterfaceId">
      <Params>
        <Param Name="Groups" Alias="GroupId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="ModifyReservedInstances" />
    <ServiceOperation MethodName="ModifySubnetAttribute"  Output="Void" PipelineParameter="PipelineParameter" PositionalParameters="MapPublicIpOnLaunch"/>
    <ServiceOperation MethodName="ModifySnapshotAttribute" Output="Void" PipelineParameter="SnapshotId">
      <Params>
        <Param Name="GroupNames" Alias="UserGroup" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="ModifyVolumeAttribute" Output="Void" PipelineParameter="VolumeId" PositionalParameters="AutoEnableIO"/>
    <ServiceOperation MethodName="ModifyVpcAttribute" Exclude="true"/>
    <ServiceOperation MethodName="ModifyVpcEndpoint" PipelineParameter="VpcEndpointId" Output="Void"/>
    <ServiceOperation MethodName="MonitorInstances" Verb="Start" Noun="InstanceMonitoring" />
    <ServiceOperation MethodName="MoveAddressToVpc" Output="Response"/>
    <ServiceOperation MethodName="PurchaseReservedInstancesOffering" Verb="New" Noun="ReservedInstance"
                      PipelineParameter="ReservedInstancesOfferingId" PositionalParameters="InstanceCount;LimitPrice_Amount;LimitPrice_CurrencyCode"/>
    <ServiceOperation MethodName="RebootInstances" Output="Void" />
