    <ServiceOperation MethodName="ReplaceRouteTableAssociation" PipelineParameter="AssociationId" PositionalParameters="RouteTableId"/>
    <ServiceOperation MethodName="ReportInstanceStatus" Output="Void" PipelineParameter="Instances" PositionalParameters="Status;StartTime;EndTime;ReasonCodes;Description">
      <Params>
        <Param Name="Instances" Alias="InstanceId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="RequestSpotInstances" ShouldProcessTarget="SpotPrice"/>
    <ServiceOperation MethodName="ResetImageAttribute" Output="Void" PipelineParameter="ImageId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="ResetInstanceAttribute" Output="Void" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="ResetNetworkInterfaceAttribute" Output="Void" PipelineParameter="NetworkInterfaceId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="ResetSnapshotAttribute" Output="Void" PipelineParameter="SnapshotId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="RestoreAddressToClassic" Output="Response"/>
