    <ServiceOperation MethodName="ReplaceNetworkAclAssociation" PipelineParameter="AssociationId" PositionalParameters="NetworkAclId"/>
    <ServiceOperation MethodName="ReplaceNetworkAclEntry" Output="Void" PipelineParameter="NetworkAclId">
