    <ServiceOperation MethodName="AcceptVpcPeeringConnection" Verb="Approve" />
    <ServiceOperation MethodName="RejectVpcPeeringConnection" Verb="Deny" Output="Void"  />
    <ServiceOperation MethodName="DeleteVpcPeeringConnection" Output="Void"  />

