    <Map From="Regions" To="Region" />
    <Map From="ReservedInstances" To="ReservedInstance" />
    <Map From="ReservedInstancesListings" To="ReservedInstancesListing" />
    <Map From="ReservedInstancesOfferings" To="ReservedInstancesOffering" />
