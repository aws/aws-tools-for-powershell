<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>EC2</ServiceNounPrefix>
  <ServiceName>Amazon Elastic Compute Cloud</ServiceName>
  <ServiceClientInterface>IAmazonEC2</ServiceClientInterface>
  <ServiceClient>AmazonEC2Client</ServiceClient>
  <ServiceNamespace>Amazon.EC2</ServiceNamespace>

  <Params>
    <Param Name="DryRun" Exclude="true" />
  </Params>

  <PositionalParameters>InstanceId</PositionalParameters>
  <PipelineParameter>InstanceId</PipelineParameter>

  <ParamEmitters>
    <ParamEmitter ParamType="System.Collections.Generic.List`1&lt;System.String&gt;"
                  ParamName="InstanceIds"
                  EmitterType="Generators.ParamEmitters.AmazonEC2.InstanceIdsParamEmitter" />
  </ParamEmitters>

  <ServiceOperations>
    <ServiceOperation MethodName="AllocateAddress" Output="Response" />
    <ServiceOperation MethodName="AssignPrivateIpAddresses" Output="Void" PipelineParameter="NetworkInterfaceId"
                      PositionalParameters="SecondaryPrivateIpAddressCount;AllowReassignment;PrivateIpAddresses"/>
    <ServiceOperation MethodName="AssociateAddress" PositionalParameters="PublicIp;AllocationId;NetworkInterfaceId"/>
    <ServiceOperation MethodName="AssociateDhcpOptions" Output="Void" PipelineParameter="VpcId" PositionalParameters="DhcpOptionsId"/>
    <ServiceOperation MethodName="AssociateRouteTable" PipelineParameter="RouteTableId" PositionalParameters="SubnetId"/>
    <ServiceOperation MethodName="AttachInternetGateway" Output="Void" PipelineParameter="VpcId" PositionalParameters="InternetGatewayId"/>
    <ServiceOperation MethodName="AttachNetworkInterface" PositionalParameters="NetworkInterfaceId;InstanceId;DeviceIndex"/>
    <ServiceOperation MethodName="AttachVolume" PositionalParameters="VolumeId;InstanceId;Device"/>
    <ServiceOperation MethodName="AttachVpnGateway" PipelineParameter="VpcId" PositionalParameters="VpnGatewayId"/>
    <ServiceOperation MethodName="AuthorizeSecurityGroupEgress" Output="Void" PipelineParameter="GroupId" PositionalParameters="IpPermissions"/>
    <ServiceOperation MethodName="AuthorizeSecurityGroupIngress" Output="Void" PipelineParameter="GroupId"/>
    <ServiceOperation MethodName="BundleInstance" Verb="New" Noun="InstanceBundle" PositionalParameters="Storage_S3_Bucket;Storage_S3_Prefix"/>
    <ServiceOperation MethodName="CancelBundleTask" />
    <ServiceOperation MethodName="CancelConversionTask" Exclude="true" />
    <ServiceOperation MethodName="CancelExportTask" Output="Void" />
    <ServiceOperation MethodName="CancelReservedInstancesListing" />
    <ServiceOperation MethodName="CancelSpotInstanceRequests" />
    <ServiceOperation MethodName="ConfirmProductInstance" PositionalParameters="ProductCode;InstanceId" Output="Response"/>
    <ServiceOperation MethodName="CopyImage" PipelineParameter="SourceImageId" PositionalParameters="SourceRegion;Description;Name;ClientToken"/>
    <ServiceOperation MethodName="CopySnapshot" PipelineParameter="SourceSnapshotId" PositionalParameters="SourceRegion;Description"/>
    <ServiceOperation MethodName="CreateCustomerGateway" PipelineParameter="Type" PositionalParameters="PublicIp;BgpAsn">
      <Params>
        <Param Name="PublicIp" Alias="IpAddress" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateDhcpOptions" />
	<ServiceOperation MethodName="CreateFlowLogs" Output="Response" PipelineParameter="LogGroupName"/>
    <ServiceOperation MethodName="CreateImage" PositionalParameters="Name;Description;NoReboot;BlockDeviceMappings"/>
    <ServiceOperation MethodName="CreateInstanceExportTask" PositionalParameters="TargetEnvironment;Description"/>
    <ServiceOperation MethodName="CreateInternetGateway"/>
    <ServiceOperation MethodName="CreateKeyPair" />
    <ServiceOperation MethodName="CreateNetworkAcl" />
    <ServiceOperation MethodName="CreateNetworkAclEntry" Output="Void" PipelineParameter="NetworkAclId"/>
    <ServiceOperation MethodName="CreateNetworkInterface" PipelineParameter="SubnetId" PositionalParameters="Description;PrivateIpAddress;Groups">
      <Params>
        <Param Name="PrivateIpAddresses" NewName="PrivateIpAddressSet" />
        <Param Name="Groups" Alias="GroupId" />  <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="CreatePlacementGroup" Output="Void" PipelineParameter="GroupName" PositionalParameters="Strategy"/>
    <ServiceOperation MethodName="CreateReservedInstancesListing" PipelineParameter="ReservedInstancesId" PositionalParameters="InstanceCount;PriceSchedules;ClientToken">
      <Params>
        <Param Name="PriceSchedules" Alias="PricingSchedules" />  <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateRoute" PipelineParameter="RouteTableId" PositionalParameters="DestinationCidrBlock;GatewayId;InstanceId;NetworkInterfaceId"/>
    <ServiceOperation MethodName="CreateRouteTable" />
    <ServiceOperation MethodName="CreateSecurityGroup" PipelineParameter="GroupName" PositionalParameters="Description;VpcId">
      <Params>
        <Param Name="Description" Alias="GroupDescription" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateSnapshot" PipelineParameter="VolumeId" PositionalParameters="Description"/>
    <ServiceOperation MethodName="CreateSpotDatafeedSubscription" PipelineParameter="Bucket" PositionalParameters="Prefix"/>
    <ServiceOperation MethodName="CreateSubnet" PipelineParameter="VpcId" PositionalParameters="CidrBlock;AvailabilityZone"/>
    <ServiceOperation MethodName="CreateTags" Output="Void" PipelineParameter="Resources" PositionalParameters="Tags">
      <Params>
        <Param Name="Resources" Alias="ResourceId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
      <PassThru>
        <Expression>cmdletContext.Tags</Expression>
        <Documentation>Returns the collection of Tag objects that were created</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateVolume" PipelineParameter="SnapshotId" PositionalParameters="Size;AvailabilityZone;IOPS;VolumeType;Encrypted"/>
    <ServiceOperation MethodName="CreateVpc" PipelineParameter="CidrBlock" PositionalParameters="InstanceTenancy"/>
    <ServiceOperation MethodName="CreateVpnConnection" PipelineParameter="Type"
                      PositionalParameters="CustomerGatewayId;VpnGatewayId;Options_StaticRoutesOnly"
                      ShouldProcessTarget="VpnGatewayId">
      <Params>
        <Param Name="Options_StaticRoutesOnly" Alias="StaticRoutesOnly" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="CreateVpnConnectionRoute" Output="Void" PipelineParameter="VpnConnectionId" PositionalParameters="DestinationCidrBlock"/>
    <ServiceOperation MethodName="CreateVpcEndpoint" PipelineParameter="VpcId" Output="Response"/>
    <ServiceOperation MethodName="CreateVpnGateway" PipelineParameter="Type" PositionalParameters="AvailabilityZone"/>
    <ServiceOperation MethodName="DeleteCustomerGateway" Output="Void" ShouldProcessMsgNoun="Customer Gateway" />
    <ServiceOperation MethodName="DeleteDhcpOptions" Output="Void" />
	<ServiceOperation MethodName="DeleteFlowLogs" />
    <ServiceOperation MethodName="DeleteInternetGateway" Output="Void" ShouldProcessMsgNoun="Internet Gateway" />
    <ServiceOperation MethodName="DeleteKeyPair" Output="Void" />
    <ServiceOperation MethodName="DeleteNetworkAcl" Output="Void" ShouldProcessMsgNoun="Network ACL" />
    <ServiceOperation MethodName="DeleteNetworkAclEntry" Output="Void" ShouldProcessTarget="NetworkAclId"  ShouldProcessMsgNoun="Network ACL Entry"
                       PipelineParameter="NetworkAclId" PositionalParameters="RuleNumber;Egress"/>
    <ServiceOperation MethodName="DeleteNetworkInterface" Output="Void" ShouldProcessMsgNoun="Network Interface" />
    <ServiceOperation MethodName="DeletePlacementGroup" Output="Void" ShouldProcessMsgNoun="Placement Group" />
    <ServiceOperation MethodName="DeleteRoute" Output="Void" ShouldProcessTarget="RouteTableId"
                      PipelineParameter="RouteTableId" PositionalParameters="DestinationCidrBlock"/>
    <ServiceOperation MethodName="DeleteRouteTable" Output="Void" ShouldProcessMsgNoun="Route Table" />
    <ServiceOperation MethodName="DeleteSecurityGroup" Output="Void" ShouldProcessMsgNoun="Security Group" ShouldProcessTarget="GroupId|GroupName"
                      PipelineParameter="GroupId"/>
    <ServiceOperation MethodName="DeleteSnapshot" Output="Void" />
    <ServiceOperation MethodName="DeleteSpotDatafeedSubscription" Output="Void" ShouldProcessMsgNoun="Spot Datafeed Subscription" />
    <ServiceOperation MethodName="DeleteSubnet" Output="Void" />
    <ServiceOperation MethodName="DeleteTags" Output="Void" ShouldProcessTarget="Resources" PipelineParameter="Resources" PositionalParameters="Tags">
      <Params>
        <Param Name="Resources" Alias="ResourceId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
      <PassThru>
        <Expression>cmdletContext.Tags</Expression>
        <Documentation>Returns the collection of Tag objects that were deleted</Documentation>
      </PassThru>
    </ServiceOperation>
    <ServiceOperation MethodName="DeleteVolume" Output="Void" />
    <ServiceOperation MethodName="DeleteVpc" Output="Void" />
    <ServiceOperation MethodName="DeleteVpnConnection" Output="Void" ShouldProcessMsgNoun="VPN Connection" />
    <ServiceOperation MethodName="DeleteVpnConnectionRoute" Output="Void" ShouldProcessTarget="VpnConnectionId" ShouldProcessMsgNoun="VPN Connection Route"
                       PipelineParameter="VpnConnectionId" PositionalParameters="DestinationCidrBlock"/>
    <ServiceOperation MethodName="DeleteVpcEndpoints" Noun="VpcEndpoint"/>
    <ServiceOperation MethodName="DeleteVpnGateway" Output="Void" ShouldProcessMsgNoun="VPN Gateway" />
    <ServiceOperation MethodName="DeregisterImage" Output="Void" />
    <ServiceOperation MethodName="DescribeAccountAttributes" >
      <Params>
        <Param Name="AttributeNames" Alias="AccountAttributeNames" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="DescribeAddresses" PipelineParameter="PublicIps" PositionalParameters="AllocationIds;Filters"/>
    <ServiceOperation MethodName="DescribeAvailabilityZones" PipelineParameter="ZoneNames" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeBundleTasks" PipelineParameter="BundleIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeConversionTasks" Exclude="true" />
    <ServiceOperation MethodName="DescribeCustomerGateways" PipelineParameter="CustomerGatewayIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeDhcpOptions" PipelineParameter="DhcpOptionsIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeExportTasks" />
	<ServiceOperation MethodName="DescribeFlowLogs" PipelineParameter="FlowLogIds"/>
    <ServiceOperation MethodName="DescribeImageAttribute" PipelineParameter="ImageId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeImages" PipelineParameter="ImageIds" PositionalParameters="Owners;ExecutableUsers;Filters">
      <Params>
        <Param Name="ExecutableUsers" Alias="ExecutableBy" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="DescribeInstanceAttribute" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeInstances" Exclude="true"/>
    <ServiceOperation MethodName="DescribeInstanceStatus" PipelineParameter="InstanceIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeInternetGateways" PipelineParameter="InternetGatewayIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeKeyPairs" PipelineParameter="KeyNames" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeMovingAddresses" Noun="MovingAddress" PipelineParameter="PublicIps" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeNetworkAcls" PipelineParameter="NetworkAclIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeNetworkInterfaceAttribute" Output="Response" PipelineParameter="NetworkInterfaceId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeNetworkInterfaces" PipelineParameter="NetworkInterfaceIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribePlacementGroups" PipelineParameter="GroupNames" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribePrefixLists" Noun="PrefixList" PipelineParameter="PrefixListIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeRegions" PipelineParameter="RegionNames" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeReservedInstances" PipelineParameter="ReservedInstancesIds" PositionalParameters="Filters;OfferingType"/>
    <ServiceOperation MethodName="DescribeReservedInstancesListings" PipelineParameter="ReservedInstancesListingId" PositionalParameters="ReservedInstancesId;Filters"/>
    <ServiceOperation MethodName="DescribeReservedInstancesModifications" />
    <ServiceOperation MethodName="DescribeReservedInstancesOfferings" PipelineParameter="ReservedInstancesOfferingIds" PositionalParameters="InstanceType;AvailabilityZone">
      <Params>
        <Param Name="ReservedInstancesOfferingIds" Alias="ReservedInstancesId" /> <!-- maintain compat with v1 sdk name -->
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="DescribeRouteTables" PipelineParameter="RouteTableIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeSecurityGroups" PipelineParameter="GroupIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeSnapshotAttribute" Output="Response" PipelineParameter="SnapshotId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeSnapshots" PipelineParameter="SnapshotIds" PositionalParameters="OwnerIds;RestorableByUserIds;Filters"/>
    <ServiceOperation MethodName="DescribeSpotDatafeedSubscription"/>
    <ServiceOperation MethodName="DescribeSpotInstanceRequests" PipelineParameter="SpotInstanceRequestIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeSpotPriceHistory"/>
    <ServiceOperation MethodName="DescribeSubnets" PipelineParameter="SubnetIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeTags" />
    <ServiceOperation MethodName="DescribeVolumeAttribute" Output="Response" PipelineParameter="VolumeId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeVolumes" PipelineParameter="VolumeIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeVolumeStatus" PipelineParameter="VolumeIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeVpcAttribute" Output="Response" PipelineParameter="VpcId" PositionalParameters="Attribute"/>
    <ServiceOperation MethodName="DescribeVpcEndpoints" Noun="VpcEndpoint" PipelineParameter="VpcEndpointIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeVpcEndpointServices" Noun="VpcEndpointService"/>
    <ServiceOperation MethodName="DescribeVpcs" PipelineParameter="VpcIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeVpnConnections" PipelineParameter="VpnConnectionIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DescribeVpnGateways" PipelineParameter="VpnGatewayIds" PositionalParameters="Filters"/>
    <ServiceOperation MethodName="DetachInternetGateway" Output="Void" PipelineParameter="VpcId" PositionalParameters="InternetGatewayId"/>
    <ServiceOperation MethodName="DetachNetworkInterface" Output="Void" PipelineParameter="AttachmentId">
      <Params>
        <Param Name="Force" NewName="ForceDismount" AutoApplyAlias="false"/>
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="DetachVolume" PositionalParameters="VolumeId;InstanceId;Device">
      <Params>
        <Param Name="Force" NewName="ForceDismount" AutoApplyAlias="false"/>
      </Params>
    </ServiceOperation>
    <ServiceOperation MethodName="DetachVpnGateway" Output="Void" PipelineParameter="VpcId" PositionalParameters="VpnGatewayId"/>
    <ServiceOperation MethodName="DisableVgwRoutePropagation" Output="Void" PipelineParameter="RouteTableId" PositionalParameters="GatewayId"/>
    <ServiceOperation MethodName="DisassociateAddress" Output="Void" PipelineParameter="PublicIp" PositionalParameters="AssociationId"/>
    <ServiceOperation MethodName="DisassociateRouteTable" Output="Void" />
    <ServiceOperation MethodName="EnableVgwRoutePropagation" Output="Void" PipelineParameter="RouteTableId" PositionalParameters="GatewayId"/>
    <ServiceOperation MethodName="EnableVolumeIO" Output="Void" />
    <ServiceOperation MethodName="GetConsoleOutput" Output="Response"/>
    <ServiceOperation MethodName="GetPasswordData" Exclude="true"/>
    <ServiceOperation MethodName="ImportInstance" Exclude="true"/>
    <ServiceOperation MethodName="ImportKeyPair" Output="Response" PipelineParameter="KeyName" PositionalParameters="PublicKeyMaterial"/>
    <ServiceOperation MethodName="ImportVolume" Exclude="true"/>

    <!-- Import V2 (these replace the V1 Import-EC2Instance and Import-EC2Volume, which are documented as deprecated) -->
    <ServiceOperation MethodName="CancelImportTask" PipelineParameter="ImportTaskId" Output="Response" />
    <ServiceOperation MethodName="DescribeImportImageTasks" Noun="ImportImageTask" PipelineParameter="ImportTaskIds" />
    <ServiceOperation MethodName="DescribeImportSnapshotTasks" Noun="ImportSnapshotTask" PipelineParameter="ImportTaskIds" />
    <ServiceOperation MethodName="ImportImage" NoPipelineParameter="true" Output="Response"/>
    <ServiceOperation MethodName="ImportSnapshot" NoPipelineParameter="true" Output="Response">
      <Params>
        <Param Name="DiskContainer_UserBucket_S3Bucket" NewName="DiskContainer_S3Bucket" />
        <Param Name="DiskContainer_UserBucket_S3Key" NewName="DiskContainer_S3Key" />
      </Params>
    </ServiceOperation>

    <!-- Spot Fleet -->
    <ServiceOperation MethodName="CancelSpotFleetRequests"
                      Noun="SpotFleetRequest"
                      Output="Response"
                      PipelineParameter="SpotFleetRequestIds" />
    <ServiceOperation MethodName="DescribeSpotFleetInstances"
                      Noun="SpotFleetInstance"
                      PipelineParameter="SpotFleetRequestId" />
    <ServiceOperation MethodName="DescribeSpotFleetRequestHistory"
                      Output="Response"
                      PipelineParameter="SpotFleetRequestId" />
