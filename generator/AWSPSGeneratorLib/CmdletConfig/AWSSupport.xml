<?xml version="1.0" encoding="utf-8" ?>
<ConfigModel 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ServiceNounPrefix>ASA</ServiceNounPrefix>
  <ServiceName>AWS Support API</ServiceName>
  <ServiceClientInterface>IAmazonAWSSupport</ServiceClientInterface>
  <ServiceClient>AmazonAWSSupportClient</ServiceClient>
  <ServiceNamespace>Amazon.AWSSupport</ServiceNamespace>

  <DefaultRegion>us-east-1</DefaultRegion>
  
  <AutoIterate Start="NextToken" EmitLimit="MaxResults" Next="NextToken" />
  
  <PositionalParameters>CaseId</PositionalParameters>
  
  <PipelineParameter>CaseId</PipelineParameter>
  
  <PipelineByValueProperties>
    <Property>CaseId</Property>
    <Property>DisplayId</Property>
    <Property>CheckId</Property>
  </PipelineByValueProperties>
  
  <ServiceOperations>
    <ServiceOperation MethodName="AddCommunicationToCase" PositionalParameters="CommunicationBody;CcEmailAddresses" />
    <ServiceOperation MethodName="CreateCase" PipelineParameter="Subject" PositionalParameters="ServiceCode;SeverityCode;CategoryCode;CommunicationBody" />
    <ServiceOperation MethodName="DescribeCases" PipelineParameter="CaseIdList" Noun="Case" LegacyAlias="Get-ASACases"/>
    <ServiceOperation MethodName="DescribeCommunications" Noun="Communication" LegacyAlias="Get-ASACommunications"/>
    <ServiceOperation MethodName="DescribeServices" PipelineParameter="ServiceCodeList" PositionalParameters="Language" Noun="Service" LegacyAlias="Get-ASAServices"/>
    <ServiceOperation MethodName="DescribeSeverityLevels" Noun="SeverityLevel" LegacyAlias="Get-ASASeverityLevels"/>
    <ServiceOperation MethodName="DescribeTrustedAdvisorCheckRefreshStatuses" Noun="TrustedAdvisorCheckRefreshStatus" LegacyAlias="Get-ASATrustedAdvisorCheckRefreshStatuses"/>
    <ServiceOperation MethodName="DescribeTrustedAdvisorCheckResult" PipelineParameter="CheckId" PositionalParameters="Language"/>
    <ServiceOperation MethodName="DescribeTrustedAdvisorChecks" Noun="TrustedAdvisorCheck" LegacyAlias="Get-ASATrustedAdvisorChecks"/>
    <ServiceOperation MethodName="DescribeTrustedAdvisorCheckSummaries" Noun="TrustedAdvisorCheckSummary" LegacyAlias="Get-ASATrustedAdvisorCheckSummaries"/>
    <ServiceOperation MethodName="RefreshTrustedAdvisorCheck" Verb="Request" Noun="TrustedAdvisorCheckRefresh" />
    <ServiceOperation MethodName="ResolveCase" Output="Response" />
    <ServiceOperation MethodName="DescribeAttachment" />
    <ServiceOperation MethodName="AddAttachmentsToSet" Output="Response" PipelineParameter="AttachmentSetId" />
  </ServiceOperations>
</ConfigModel>