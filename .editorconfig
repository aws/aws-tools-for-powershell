# https://EditorConfig.org

# top-most EditorConfig file
root = true

[**]
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2

[*.sln]
indent_style = tab

[*.{config,cs,json,ps*1,proj}]
indent_size = 4

[generator/AWSPSGeneratorLib/Config/ServiceConfig/*.xml]
indent_size = 4

[generator/AWSPSGeneratorLib/**.*xml]
insert_final_newline = false

[**.{config,*proj,xsd}]
insert_final_newline = false

[LICENSE]
indent_size = 3
